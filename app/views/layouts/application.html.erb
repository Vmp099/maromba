<!DOCTYPE html>
<html>
  <head>
    <title>MrOympia Tribess</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "applications", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <div class="container">
      <%= render 'layouts/navbar' %>
      <% if notice %>
        <p class="success"><%= notice %></p>
      <% end %>
      <% if alert %>
        <p class="error"><%= alert %></p>
      <% end %>
      <section class="section">
        <%= yield %>
      </div>
    </div>
  </body>
</html>
